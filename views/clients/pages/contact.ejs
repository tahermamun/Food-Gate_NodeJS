<!-- include html head -->
<%- include('../partials/header.ejs'); %>

      <!-- include secondary hero section -->
      <%- include('../partials/heroSectionSecondary.ejs'); %>

    

        <!-- contact page main section  start-->
        <section class='pt-5'>
            <div class=' contact_page_container container'>

                <!-- contact page description start-->
                <div class="contact_page_left">
                    <h5>CONTACT</h5>
                    <h2 class="contact_title">Send me a message for business inquiries</h2>
                    <p class="contact_des">Some quick example text to build on the card title and make up the bulk
                        of the card's content.
                    <ul>
                        <li>Phone number: +8801xxxxxxxxx</li>
                        <li>Email: Info@mamzz.com</li>
                    </ul>
                    </p>
                    <h2><i>Signature</i></h2>

                </div>
                <!-- contact page description end-->




                <!-- contact form start -->
                <div class="contact_page_right">
                    <form>
                        <input type="text" class="form-control mb-2" placeholder="Your Name">
                        <input type="email" class="form-control mb-2" placeholder="Your Email">
                        <input type="text" class="form-control mb-2" placeholder="Subject">
                        <textarea type="text" rows='6' class="form-control mb-2" placeholder="Message"></textarea>
                        <button type="submit" class="btn btn-success">Submit</button>
                    </form>
                </div>
                <!-- contact form end -->
        </section>
        <!-- contact page main section end-->


        <!-- footer section include -->
        <%- include('../partials/footer.ejs')%>